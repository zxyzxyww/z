class Mysingleton:
    __obj=None
    __init_flag=True
    def __mew__(cls,*args,**kwargs):
        if cls.__obj==None:
            cls.__obj=object.__new__(cls)
        return cls.__obj

    def __init__(self,name):
        if Mysingleton.__init_flag:
            print("初始化第一个对象。。。")
            self.name=name
            Mysingleton.__init_flag=False

a=Mysingleton("aa")
print(a)
b=Mysingleton("bb")
print(b)
